function setML(foo,mlIdx,mlBool)
global Cw6device;

% if (mlIdx>2)
%     mlIdx = mlIdx + 14;
% end

Cw6device.SystemInfo.MeasurementAct(mlIdx)=mlBool;

% if (mlIdx>2)
%     mlIdx = mlIdx + 14;
% end


Cw6device.instrument.SetMLAct(mlIdx-1,mlBool);
Cw6device.samplechannels=length(find(Cw6device.SystemInfo.MeasurementAct==1));

return